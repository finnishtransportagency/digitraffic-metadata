<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:dt="http://tie.digitraffic.fi/schemas"
            xmlns:D2LogicalModel="http://datex2.eu/schema/2/2_0" elementFormDefault="qualified"
            targetNamespace="http://tie.digitraffic.fi/schemas" version="1.1">
    <xsd:import namespace="http://datex2.eu/schema/2/2_0"
                schemaLocation="DATEXIISchema_2_2_3_with_definitions_FI.xsd"/>
    <xsd:element name="TrafficDisordersDatex2">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="maxAge" type="xsd:nonNegativeInteger"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="TimestampedTrafficDisorderDatex2">
        <xsd:sequence>
            <xsd:element name="published" type="dt:ObservationTimeType"/>
            <xsd:element maxOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="TimestampedRoadworkDatex2">
        <xsd:sequence>
            <xsd:element name="published" type="dt:ObservationTimeType"/>
            <xsd:element maxOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="TimestampedWeightRestrictionDatex2">
        <xsd:sequence>
            <xsd:element name="published" type="dt:ObservationTimeType"/>
            <xsd:element maxOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ObservationTimeType">
        <xsd:sequence>
            <xsd:element name="utc" type="xsd:dateTime"/>
            <xsd:element name="localtime" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="TrafficDisordersDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="disorder" type="dt:TimestampedTrafficDisorderDatex2"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RoadworksDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="roadwork" type="dt:TimestampedRoadworkDatex2"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="WeightRestrictionsDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="restriction" type="dt:TimestampedWeightRestrictionDatex2"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="TmsStationDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TmsDataDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="VmsDataDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="VmsTableDatex2Response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="1" name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>